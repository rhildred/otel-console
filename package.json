{
  "name": "otel-console",
  "version": "1.0.4",
  "description": "a console object with readline can be hacked for otel logging",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "exports": {
    ".": {
      "types": "./dist/Console.d.ts",
      "import": "./dist/Console.js",
      "require": "./dist/Console.cjs"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "readline-sync": "^1.4.10",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "tsup": {
    "dts": true,
    "bundle": false,
    "treeshake": true,
    "target": "node18",
    "format": ["esm", "cjs"],
    "entry": ["lib/*.ts"]
  }

}
